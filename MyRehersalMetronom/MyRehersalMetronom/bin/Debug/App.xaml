<Application
    x:Class="MyRehersalMetronom.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MyRehersalMetronom">

    <Application.Resources>
        <ResourceDictionary>
            <x:String x:Key="AppName">My Rehersal Metronom</x:String>
            <x:String x:Key="RehersalPageName">Rehersal</x:String>
            <x:String x:Key="TimeBarPageName">Time Bar</x:String>
            <ResourceDictionary.MergedDictionaries>

                <!-- 
                    Styles that define common aspects of the platform look and feel
                    Required by Visual Studio project and item templates
                 -->
                <ResourceDictionary Source="Common/StandardStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
</Application>

